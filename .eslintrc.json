{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "plugin:react/recommended",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-debugger": 0,
        "no-alert": 0,
        "no-console": 0,
        "no-unused-vars": 1,
        "react/prop-types": 0,
        "react/no-unescaped-entities": 0,
        "react/forbid-prop-types": 1
    }
}
